<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.earth.mapper.MapMapper">
		<insert id="insertPin" parameterType="MapVo">
			insert into mapPin1 (name,latitude,longitude,pinType) values(#{name},#{latitude},#{longitude},#{type})
		</insert>
<!--	a1 불러오기-->
	<select id="selectPin" parameterType="String" resultType="hashMap">
			select * from mapPin1 where pintype=${value}
	</select>
</mapper>





